<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="description" content="Manup Template">
        <meta name="keywords" content="Manup, unica, creative, html">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>{% block title %}Welcome!{% endblock %}</title>

        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_link_tags('font-awesome') }}
            <link href="https://fonts.googleapis.com/css?family=Work+Sans:400,500,600,700,800,900&display=swap" rel="stylesheet">
            <link href="https://fonts.googleapis.com/css?family=Poppins:400,500,600,700&display=swap" rel="stylesheet">

            <link rel="stylesheet" href="{{ asset("custom/css/bootstrap.min.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("custom/css/bootstrap.min.css") }}" type="text/css">
            {# <link rel="stylesheet" href="{{ asset("custom/css/font-awesome.min.css") }}" type="text/css"> #}
            <link rel="stylesheet" href="{{ asset("custom/css/elegant-icons.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("custom/css/owl.carousel.min.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("custom/css/magnific-popup.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("custom/css/slicknav.min.css") }}" type="text/css">
            <link rel="stylesheet" href="{{ asset("custom/css/style.css") }}" type="text/css">
        {% endblock %}

    </head>
    <body>
        {{ include('header.html.twig') }}
        {% block body %}
        {% endblock %}
        {{ include('footer.html.twig') }}

        {% block javascripts %}
            {# {{ encore_entry_script_tags('app') }} #}
            <script src="{{ asset("custom/js/jquery-3.3.1.min.js") }}"></script>
            <script src="{{ asset("custom/js/bootstrap.min.js") }}"></script>
            <script src="{{ asset("custom/js/jquery.magnific-popup.min.js") }}"></script>
            <script src="{{ asset("custom/js/jquery.countdown.min.js") }}"></script>
            <script src="{{ asset("custom/js/jquery.slicknav.js") }}"></script>
            <script src="{{ asset("custom/js/owl.carousel.min.js") }}"></script>
            <script src="{{ asset("custom/js/main.js") }}"></script>
        {% endblock %}
    </body>
</html>
